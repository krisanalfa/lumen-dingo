{
    "name": "krisanalfa/lumen-dingo",
    "description": "The Laravel Lumen Framework with Dingo API.",
    "keywords": ["framework", "laravel", "lumen", "dingo", "api"],
    "license": "MIT",
    "type": "project",
    "require": {
        "php": ">=5.5.9",
        "laravel/lumen-framework": "5.2.*",
        "vlucas/phpdotenv": "~2.2",
        "tymon/jwt-auth": "^0.5.9",
        "illuminate/routing": "^5.2",
        "illuminate/hashing": "^5.2",
        "dingo/api": "1.0.x@dev"
    },
    "require-dev": {
        "fzaninotto/faker": "~1.4",
        "phpunit/phpunit": "~4.0",
        "symfony/var-dumper": "^3.0"
    },
    "autoload": {
        "psr-4": {
            "App\\": "app/"
        }
    },
    "autoload-dev": {
        "classmap": [
            "tests/",
            "database/"
        ],
        "files": [
            "app/helpers.php"
        ]
    }
}
